install-glade_helpDATA: $(glade_help_DATA)
	@$(NORMAL_INSTALL)
	@list='$(glade_help_DATA)'; test -n "$(glade_helpdir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(glade_helpdir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(glade_helpdir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(glade_helpdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(glade_helpdir)" || exit $$?; \
	done

uninstall-glade_helpDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(glade_help_DATA)'; test -n "$(glade_helpdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(glade_helpdir)'; $(am__uninstall_files_from_dir)
